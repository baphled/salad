<ul>
	<% @list.each do |file| %>
	  <li>
			<%=h file[:file] %><br/>
			<span><%=h file[:feature_line] %></span><br/>
			<p><%=h file[:in_order] %></p>
			<p><%=h file[:as_a] %></p>
			<p><%=h file[:i_want] %></p>
			<p>
				<% file[:scenarios].each do |scenario| %>
					<%=h scenario %><br/>
				<% end %>
			</p>
		</li>
	<% end %>
</ul>