<span class="icons">
	<%= link_to "View", project_path(project)%>
	<%= link_to "Edit", edit_project_path(project)%>
	<%= link_to 'Delete', project, :confirm => 'Are you sure?', :method => :delete %>
	<% if !project.feature_ids.empty? %>
		<%= link_to "Features", project_features_path(project)%>
	<% end %>
	<%= link_to "New Feature", new_feature_path({:project_id=>project.id}) %>
</span>